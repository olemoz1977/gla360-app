{
  "name": "gla360-app",
  "version": "1.2.0",
  "private": true,
  "description": "GLA360 â€” React 19 + Vite 7 + TS, Vitest, ESLint, Prettier",
  "type": "module",
  "engines": {
    "node": ">=20.19",
    "npm": ">=9"
  },
  "packageManager": "npm@10",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.4",
    "@eslint/js": "9.37.0",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "eslint": "9.37.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vitest": "3.2.3"
  },
  "overrides": {
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9"
  }
}
